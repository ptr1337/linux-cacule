diff --git a/init/Kconfig b/init/Kconfig
index afd708ed0..258cd48cc 100644
--- a/init/Kconfig
+++ b/init/Kconfig
@@ -1332,7 +1332,6 @@ config CC_OPTIMIZE_FOR_PERFORMANCE
 
 config CC_OPTIMIZE_FOR_PERFORMANCE_O3
 	bool "Optimize more for performance (-O3)"
